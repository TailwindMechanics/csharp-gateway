{
  "workbench.colorTheme": "Monokai",
  "editor.codeLens": false,
  "editor.formatOnSave": true,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "editor.detectIndentation": false,
  "explorer.fileNesting.patterns": {
    "root": ".editorconfig, .env, .gitignore, appsettings.json, autoCommentPath.js, docker-compose.yml, Dockerfile, neurocache-gateway.csproj, neurocache-gateway.sln, Program.cs, project-hierarchy.txt, README.md, rootGrouper.js"
  },
  "material-icon-theme.folders.associations": {
    "Properties": "Utils",
    "External": "Import",
    "Internal": "Export",
    "Requests": "Upload",
    "Agents": "Robot",
    "Obj": "Packages",
    "Health": "Tools",
    "Agent": "Robot",
    "Root": "Base"
  },
  "material-icon-theme.files.associations": {
    "root": "Tree"
  },
  "project-hierarchy-explorer.ignorePatterns": [
    ".git",
    "env",
    "bin",
    "obj",
    ".vscode"
  ]
}
